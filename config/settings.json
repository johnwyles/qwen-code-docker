{
  "apiEndpoint": {
    "baseUrl": "https://avi.alliance.unm.edu:8443/v1",
    "apiKey": "",
    "timeout": 30000,
    "retries": 3
  },
  "model": {
    "name": "qwen2.5-coder:32b",
    "temperature": 0.1,
    "maxTokens": 4096,
    "topP": 0.9,
    "frequencyPenalty": 0.0,
    "presencePenalty": 0.0
  },
  "session": {
    "contextWindow": 8192,
    "maxHistory": 50,
    "preserveHistory": true,
    "autoSave": true,
    "saveInterval": 300
  },
  "interface": {
    "theme": "dark",
    "language": "en",
    "showLineNumbers": true,
    "syntaxHighlighting": true,
    "autoComplete": true,
    "wordWrap": true
  },
  "development": {
    "debug": false,
    "verbose": false,
    "logLevel": "info",
    "logFile": "/workspace/.config/qwen-code/logs/qwen-code.log"
  },
  "workspace": {
    "defaultPath": "/workspace/code",
    "autoDetectLanguage": true,
    "fileExtensions": [
      ".js", ".ts", ".jsx", ".tsx",
      ".py", ".go", ".rs", ".java",
      ".cpp", ".c", ".h", ".hpp",
      ".php", ".rb", ".swift", ".kt",
      ".html", ".css", ".scss", ".sass",
      ".json", ".yaml", ".yml", ".xml",
      ".md", ".txt", ".sh", ".bash"
    ],
    "excludePatterns": [
      "node_modules",
      ".git",
      "dist",
      "build",
      ".cache",
      "*.log"
    ]
  },
  "features": {
    "codeGeneration": true,
    "codeReview": true,
    "debugging": true,
    "refactoring": true,
    "documentation": true,
    "testing": true
  },
  "security": {
    "sanitizeInput": true,
    "validateCode": true,
    "restrictFileAccess": false,
    "allowedDomains": [
      "avi.alliance.unm.edu"
    ]
  }
}